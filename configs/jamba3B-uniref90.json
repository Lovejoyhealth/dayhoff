{
  "dataset": "uniref90_202401",
  "task": "lm",
  "model_type": "jamba",
  "model_config": {
    "hidden_size": 1280,
    "intermediate_size": 2560,
    "num_hidden_layers": 32,
    "num_attention_heads": 32,
    "num_key_value_heads": 8,
    "use_mamba_kernels": true,
    "mamba_d_state": 16,
    "mamba_d_conv": 4,
    "mamba_expand": 2,
    "mamba_dt_rank": "auto",
    "mamba_conv_bias": true,
    "mamba_proj_bias": false,
    "output_router_logits": true,
    "use_cache": false
  },
  "aux_loss_weight": 0.001,
  "epochs": 100,
  "max_tokens": 90000,
  "max_len": 2048,
  "pad_to_multiple_of": 8,
  "max_batch_size": 6000,
  "bucket_size": 1000,
  "lr": 4e-4,
  "warmup_steps": 16000,
  "flip_prob": 0.5,
  "activation_checkpointing": 0
}
